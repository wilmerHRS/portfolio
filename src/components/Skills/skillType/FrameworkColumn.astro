// import { BsFillBootstrapFill } from "react-icons/bs";
import { Icon } from "astro-icon";

import Column from "../Column.astro";
import SkillItem from "../SkillItem.astro";
---

<Column title="Frameworks">
  <SkillItem name="Next.js" color="#FFFFFF">
    <!-- <SiNextdotjs size={22} client:load /> -->
    <Icon name="akar-icons:nextjs-fill" class="w-6 h-6" />
  </SkillItem>
  <SkillItem name="Astro" color="#FF8700">
    <!-- <SiAstro size={22} client:load /> -->
    <Icon name="simple-icons:astro" class="w-6 h-6" />
  </SkillItem>
  <SkillItem name="Express" color="#6BBB3B">
    <!-- <SiExpress size={22} client:load /> -->
    <Icon name="simple-icons:express" class="w-6 h-6" />
  </SkillItem>
  <SkillItem name="Nest" color="#EE3363">
    <!-- <SiNestjs size={22} client:load /> -->
    <Icon name="simple-icons:nestjs" class="w-6 h-6" />
  </SkillItem>
  <SkillItem name=".NET Core" color="#8C3DCF">
    <!-- <SiDotnet size={22} client:load /> -->
    <Icon name="simple-icons:dotnet" class="w-6 h-6" />
  </SkillItem>
  <SkillItem name="Flask" color="#FFFFFF">
    <!-- <SiFlask size={22} client:load /> -->
    <Icon name="simple-icons:flask" class="w-6 h-6" />
  </SkillItem>
  <SkillItem name="Bootstrap" color="#6D5CFF">
    <!-- <BsFillBootstrapFill size={22} client:load /> -->
    <Icon name="simple-icons:bootstrap" class="w-6 h-6" />
  </SkillItem>
